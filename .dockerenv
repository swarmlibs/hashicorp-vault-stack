# Vault deployment
VAULT_STACK_IMAGE=hashicorp/vault
VAULT_STACK_IMAGE_TAG=1.17.2

# Enable Vault Telemetry (vault-telemetry.yml)
VAULT_STACK_TELEMETRY_ENABLED=true
VAULT_STACK_EXTRACONFIG_VERSION=v1

# Docker Service's task replicas
VAULT_TASK_REPLICAS=1

# Vault Configuration
# https://developer.hashicorp.com/vault/docs/configuration
# VAULT_LOG_LEVEL=warn
# VAULT_API_ADDR=http://replica-{{.Task.Slot}}.vault-internal:8200
